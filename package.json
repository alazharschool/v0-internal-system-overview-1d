    "education",
    "school-management",
    "online-learning",
    "quran",
    "islamic-education",
    "nextjs",
    "typescript",
    "tailwindcss",
    "supabase"
  ],
  "homepage": "https://github.com/your-username/alazhar-school-system#readme",
