{
  "name": "alazhar-school-system",
  "version": "1.0.0",
  "private": true,
  "description": "Educational management system for Al-Azhar Online School",
  "keywords": [
    "education",
    "school-management",
    "online-learning",
    "quran",
    "islamic-education",
    "nextjs",
    "typescript",
    "tailwindcss",
    "supabase"
  ],
  "homepage": "https://github.com/your-username/alazhar-school-system#readme",
  "bugs": {
    "url": "https://github.com/your-username/alazhar-school-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/alazhar-school-system.git"
  },
  "license": "MIT",
  "author": "Al-Azhar Online School",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "clean": "rm -rf .next out dist",
    "db:generate": "supabase gen types typescript --project-id your-project-id --schema public > lib/database.types.ts",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed.ts",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preinstall": "npx only-allow npm",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "verify": "tsx scripts/verify-setup.ts"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "latest",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.49.2",
    "@tanstack/react-query": "^5.67.1",
    "@tanstack/react-table": "^8.21.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.15.0",
    "immer": "latest",
    "lucide-react": "^0.469.0",
    "next": "15.5.9",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "use-sync-external-store": "latest",
    "vaul": "^1.1.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.7",
    "eslint-config-prettier": "^9.1.0",
